                var step = $inputQty.attr('step');
                step = 'undefined' !== typeof (step) ? parseInt(step) : 1;
                if (val > 0) {
                    $inputQty.val(val - step).change();
                }
            });
            $('.woocommerce .quantity').on('click', '.plus', function (e) {
                var $inputQty = $(this).parent().find('input.qty');
